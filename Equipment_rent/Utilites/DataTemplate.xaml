<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Equipment_rent.ViewModel"
                    xmlns:view="clr-namespace:Equipment_rent.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:EquipmentsVM}">
        <view:Equipments/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrdersVM}">
        <view:Orders/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:StaffVM}">
        <view:Staff/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddStaffVM}">
        <view:AddStaff/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:EditStaffVM}">
        <view:EditStaff/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UsersVM}">
        <view:Users/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ProfileVM}">
        <view:Profile/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddUserVM}">
        <view:AddUser/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddOrderVM}">
        <view:AddOrder/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddEquipmentVM}">
        <view:AddEquipment/>
    </DataTemplate>

</ResourceDictionary>